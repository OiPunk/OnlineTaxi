<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.rydr.taxi.dao.mapper.ServiceSmsTemplateCustomMapper" >

    <!-- Query template by templateId -->
    <select id="selectByTemplateId" resultMap="com.rydr.taxi.dao.mapper.ServiceSmsTemplateMapper.BaseResultMap" parameterType="java.lang.String" >
        select
        <include refid="com.rydr.taxi.dao.mapper.ServiceSmsTemplateMapper.Base_Column_List" />
        from service_sms_template
        where template_id = #{templateId}
    </select>

</mapper>